package password_programm;

public class passwordProgramm {
	
	
	// i have 3 numbers , 6 digits
	// need to get all possible numbers from the 3 numbers in the 6 digits.
	//the steps:-
	//put the 3 numbers in array
	//apply mathematics approach to get all possible numbers
	
	static int  array_of_given_numbers[]=new int[3];
	static int[] myresult_Array;

	public static void main(String[] args) {
		give_me_your_3_numbers(array_of_given_numbers);
		System.out.println(array_of_given_numbers);

	}
	
	public static int[] give_me_your_3_numbers(int [] arr) {
		arr[0]=1;
		arr[1]=2;
		arr[2]=3;
		
	return arr;
	}

	
	
	
	
	
	

}
